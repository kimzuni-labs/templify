{
  "name": "@kimzuni/templify",
  "description": "Flexible template string processor for JavaScript and TypeScript.",
  "version": "1.3.0",
  "license": "MIT",
  "private": false,
  "type": "module",
  "author": {
    "name": "kimzuni",
    "email": "me@zuni.kim",
    "url": "https://github.com/kimzuni"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimzuni-labs/templify.git"
  },
  "homepage": "https://github.com/kimzuni-labs/templify",
  "bugs": "https://github.com/kimzuni-labs/templify/issues",
  "scripts": {
    "test": "NODE_ENV=test tsx --test",
    "test:all": "NODE_ENV=test tsx --test tests/*.test.ts",
    "lint": "markdownlint-cli2 && eslint .",
    "lint:eslint": "eslint .",
    "lint:markdown": "markdownlint-cli2",
    "build": "tsdown",
    "prepare": "husky"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "keywords": [
    "template",
    "string",
    "render",
    "placeholder",
    "typescript"
  ],
  "devDependencies": {
    "@commitlint/cli": "20.2.0",
    "@commitlint/config-conventional": "20.2.0",
    "@eslint/js": "9.39.2",
    "@stylistic/eslint-plugin": "5.6.1",
    "@types/bun": "1.3.5",
    "eslint": "9.39.2",
    "husky": "9.1.7",
    "markdownlint-cli2": "0.20.0",
    "tsdown": "0.18.3",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.50.1"
  }
}
