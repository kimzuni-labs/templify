{
  "name": "@kimzuni/templify",
  "description": "Flexible template string processor for JavaScript and TypeScript.",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "author": {
    "name": "kimzuni",
    "email": "me@zuni.kim",
    "url": "https://github.com/kimzuni"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kimzuni-labs/templify.git"
  },
  "homepage": "https://github.com/kimzuni-labs/templify",
  "bugs": "https://github.com/kimzuni-labs/templify/issues",
  "scripts": {
    "test": "bun test",
    "lint": "bun lint:markdown && bun lint:eslint",
    "lint:eslint": "eslint .",
    "lint:markdown": "markdownlint-cli2",
    "build": "bun scripts/build.ts",
    "publish": "npm publish --provenance --access=public",
    "release": "bun run test && bun run lint && bun run build && standard-version"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.js"
    }
  },
  "keywords": [
    "template",
    "string",
    "render",
    "placeholder",
    "typescript"
  ],
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@stylistic/eslint-plugin": "^5.5.0",
    "@types/bun": "^1.3.1",
    "eslint": "^9.38.0",
    "jiti": "^2.6.1",
    "markdownlint-cli2": "^0.18.1",
    "standard-version": "^9.5.0",
    "typescript-eslint": "^8.46.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}
